{
  "name": "@monoweave/git",
  "version": "2.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/monoweave/monoweave.git",
    "directory": "packages/git"
  },
  "author": {
    "name": "noahnu",
    "url": "https://monoweave.github.io/monoweave/"
  },
  "license": "BSD-3",
  "publishConfig": {
    "main": "./lib/index.js",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "./test-mocks": null
    },
    "registry": "https://registry.npmjs.org/",
    "types": "./lib/index.d.ts",
    "access": "public"
  },
  "group": "monoweave",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./test-mocks": "./src/testMock.ts"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "run workspace:clean \"$(pwd)\"",
    "types:watch": "run workspace:types:watch \"$(pwd)\"",
    "test:watch": "run workspace:test:watch \"$(pwd)\"",
    "build:watch": "run workspace:build:watch \"$(pwd)\"",
    "prepack": "run workspace:build \"$(pwd)\""
  },
  "devDependencies": {
    "@monoweave/io": "workspace:*",
    "@monoweave/logging": "workspace:*",
    "@monoweave/test-utils": "workspace:*",
    "@monoweave/types": "workspace:*",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.18.10",
    "@yarnpkg/core": "^4.4.4",
    "@yarnpkg/fslib": "^3.1.3",
    "@yarnpkg/shell": "^4.1.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "micromatch": "^4.0.8"
  },
  "peerDependencies": {
    "@monoweave/io": "workspace:^2.0.0",
    "@monoweave/logging": "workspace:^2.0.0",
    "@monoweave/types": "workspace:^2.0.0",
    "@yarnpkg/core": "^4.4.4",
    "@yarnpkg/fslib": "^3.1.3",
    "@yarnpkg/shell": "^4.1.3"
  }
}
