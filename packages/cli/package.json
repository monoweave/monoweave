{
  "name": "@monoweave/cli",
  "version": "2.2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/monoweave/monoweave.git",
    "directory": "packages/cli"
  },
  "author": {
    "name": "noahnu",
    "url": "https://monoweave.github.io/monoweave/"
  },
  "license": "BSD-3",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "bin": {
      "monoweave": "./lib/index.js"
    },
    "main": "./lib/index.js",
    "access": "public",
    "exports": {
      ".": "./lib/index.js",
      "./preset-*": "./lib/presets/*.js"
    }
  },
  "group": "monoweave",
  "bin": {
    "monoweave": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./preset-*": "./src/presets/*.ts"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "run workspace:clean \"$(pwd)\"",
    "types:watch": "run workspace:types:watch \"$(pwd)\"",
    "test:watch": "run workspace:test:watch \"$(pwd)\"",
    "build:watch": "run workspace:build:watch \"$(pwd)\"",
    "prepack": "run workspace:build \"$(pwd)\""
  },
  "devDependencies": {
    "@monoweave/test-utils": "workspace:*",
    "@types/node": "^22.18.13",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.9.0",
    "@monoweave/changelog": "workspace:^2.2.0",
    "@monoweave/dependencies": "workspace:^2.2.0",
    "@monoweave/git": "workspace:^2.2.0",
    "@monoweave/io": "workspace:^2.2.0",
    "@monoweave/logging": "workspace:^2.2.0",
    "@monoweave/node": "workspace:^2.2.0",
    "@monoweave/types": "workspace:^2.2.0",
    "@monoweave/versions": "workspace:^2.2.0",
    "@yarnpkg/cli": "^4.12.0",
    "@yarnpkg/core": "^4.5.0",
    "@yarnpkg/fslib": "^3.1.4",
    "@yarnpkg/plugin-npm": "^3.4.0",
    "@yarnpkg/plugin-pack": "^4.0.4",
    "@yarnpkg/shell": "^4.1.3",
    "ajv": "^8.17.1",
    "clipanion": "^4.0.0-rc.4",
    "json5": "^2.2.3",
    "typanion": "^3.14.0",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": ">=v18"
  }
}
