    "@monoweave/io": "workspace:^1.12.0",
    "@monoweave/logging": "workspace:^1.12.0",
    "@monoweave/node": "workspace:^1.12.0",
    "@monoweave/types": "workspace:^1.12.0",
    "@monoweave/versions": "workspace:^1.12.0",
    "@yarnpkg/cli": "^4.5.3",
    "@yarnpkg/core": "^4.1.6",
    "@yarnpkg/fslib": "^3.1.1",
    "@yarnpkg/plugin-npm": "^3.0.1",
    "@yarnpkg/plugin-pack": "^4.0.0",
    "@yarnpkg/shell": "^4.1.1",
    "ajv": "^8.17.1",
    "clipanion": "^4.0.0-rc.4",
    "json5": "^2.2.3",
