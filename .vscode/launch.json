{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Test",
            "cwd": "${workspaceFolder}",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "yarn",
            "console": "integratedTerminal",
            "args": [
                "run",
                "--inspect",
                "vitest",
                "--config=${workspaceFolder}/vitest.config.ts",
                "--testTimeout=600000",
                "${relativeFile}"
            ]
        }
    ]
}
